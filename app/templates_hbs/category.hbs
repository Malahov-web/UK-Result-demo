<!DOCTYPE html>
<html>
<head>
    <title>UK Result demo - category</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, user-scalable=yes, initial-scale=1.0">
    <meta content='true' name='HandheldFriendly'>
    <meta content='width' name='MobileOptimized'>
    <meta content='yes' name='apple-mobile-web-app-capable'>

    <meta name="theme-color" content="#c2b385"> 
    <!-- Chrome, Firefox OS and Opera -->
    <meta name="theme-color" content="#c2b385"> 
    <!-- Windows Phone -->
    <meta name="msapplication-navbutton-color" content="#c2b385"> 
    <!-- iOS Safari -->
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="apple-mobile-web-app-status-bar-style" content="black-translucent"> 
    <!-- Яндекс.Браузер -->
    <meta name="viewport" content="ya-title=#c2b385,ya-dock=fade">      

    <link rel="stylesheet" type="text/css" href="css/normalize.css">
    <link rel="stylesheet" type="text/css" href="css/style.css">
    <!-- Plugins -->
 
</head>

<body class="category">




{{> header}}




{{> overlay-search}}




<section class="section section-category">
    <div class="container">

        <div class="row">
            <div class="mv_24">
                <h2 class="title">Новости</h2>
            </div>
        </div>

        <div class="row category news--slat">

            {{#news}}
            <div class="mv_22 mv_offset_1 tb_24 tb_offset_0 ds_24">
                <div class="news__item">

                    <a href="{{item-link}}" class="news__item-image">
                        <img src="uploads/news/{{item-image}}" alt="{{item-title}}">
                    </a>

                    <div class="news__item-content">
                        <div class="news__item-content-inner">
                            <div class="news__item-title"><a href="">{{item-title}}</a></div>
                            <time class="news__item-date" datetime="{{item-datetime}}">{{item-date}}</time>
                            <div class="news__item-text">{{item-text-long}}</div>
                        </div>
                    
                        <div class="news__item-button">
                            <a href="{{item-link}}" class="btn btn-readmore">Читать дальше</a>
                        </div>
                    </div>
                </div>
            </div>
            {{/news}}

        </div>



    </div>
</section>




{{> footer}}




{{> overlay}}




    <script src="js/min/scripts.min.js"> </script>    

</body>
</html>